<%- include("../eshop/header", {title: title, user: user}); %>

<h1 class="welcome">Customer details</h1>


<pre><%#= JSON.stringify(res, null, 4) %></pre>
<table>
    <tr>
        <th>Id</th>
        <th>Namn</th>
        <th>Address</th>
        <th>Telefon</th>
        <th>Skapa</th>
    </tr>
<% for (const row of res) { %>
    <tr>
        <td><%= row.id %></></td>
        <td><%= row.fornamn %> <%= row.efternamn %></td>
        <td><%= row.adress %></td>
        <td><%= row.telefon %></td>
        <td>
            <a class="order" href="/eshop/customer-show/<%= row.id %>"><p class="create-order">Skapa order</p></a>
        </td>
    </tr>
<% }; %>
</table>

<%- include("../eshop/footer"); %>
