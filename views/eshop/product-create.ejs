<%- include("../eshop/header", {title: title, user: user}); %>

<h1>Create new product</h1>

<p>Create a new product holder.</p>

<form class="label-left" method="post">
    <fieldset>
        <legend>Create product</legend>

        <label for="ettId">Id</label>
        <input id="ettId" type="text" name="ettId">

        <label for="ett_pris">Price</label>
        <input id="ett_pris" type="number" name="ett_pris">

        <label for="ett_prodnamn">Name</label>
        <input id="ett_prodnamn" type="text" name="ett_prodnamn">

        <label for="ett_bildlank">Bild link</label>
        <input id="ett_bildlank" type="text" name="ett_bildlank">

        <label for="en_beskrivning">Beskrivning</label>
        <input id="en_beskrivning" type="text" name="en_beskrivning">

        <input type="submit" name="doit" value="Create">
